@page
@model Projekt_Jordnaer.Pages.Medlemmer.CreateMemberModel
@{
    ViewData["Title"] = "Tilføj medlem";
}

<h1>Tilføj medlem</h1>

<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div class="form-group">
                <label asp-for="@Model.Medlem.MemberID" class="control-label"></label>
                <input asp-for="@Model.Medlem.MemberID" class="form-control" />

            </div>
            <div class="form-group">
                <label asp-for="@Model.Medlem.Name" class="control-label"></label>
                <input asp-for="@Model.Medlem.Name" class="form-control" />

            </div>
            <div class="form-group">
                <label asp-for="@Model.Medlem.Address" class="control-label"></label>
                <input asp-for="@Model.Medlem.Address" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="@Model.Medlem.Name" class="control-label"></label>
                <input asp-for="@Model.Medlem.Name" class="form-control" />
            </div>


            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

